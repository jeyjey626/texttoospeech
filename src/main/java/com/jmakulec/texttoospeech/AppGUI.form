              <component id="34c3" class="javax.swing.JMenuItem">
                <constraints/>
                <properties>
                  <text value="O Aplikacji"/>
                </properties>
              </component>
              <component id="b7088" class="javax.swing.JMenuItem">
                <constraints/>
                <properties>
                  <text value="Zamknij Alt+F4"/>
